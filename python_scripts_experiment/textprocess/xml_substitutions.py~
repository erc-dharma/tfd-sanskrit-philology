import re

def xml_substitutions(line):
        subdict ={'\\\\msCa': '<ms>C<sub>a</sub></ms>',
                '\\\\msCb': '<ms>C<sub>b</sub></ms>',
                '\\\\msCc': '<ms>C<sub>c</sub></ms>',
                '\\\\mssCaCbCc': '<ms><b>C</b></ms>',
                '\\\\msNa': '<ms>N<sub>a</sub></ms>',
                '\\\\msNb': '<ms>N<sub>b</sub></ms>',
                '\\\\msNc': '<ms>N<sub>c</sub></ms>',
                '\\\\msBod': '<ms>B</ms>',
                '\\\\msL': '<ms>L</ms>',
                '\\\\msA': '<ms>A</ms>',
                '\\\\msB': '<ms>B</ms>',
                '\\\\msC': '<ms>C</ms>',
                '\\\\msD': '<ms>D</ms>',
                '\\\\msE': '<ms>E</ms>',
                '\\\\msF': '<ms>F</ms>',
                '\\\\Ed': '<ms>Ed</ms>',
                '\\\\om': '<i>omitted in</i>',
                '\\\\eme': '<i>eme.</i>',
                '\\\\conj': '<i>conj.</i>',
                '\\\\corr': '<i>corr.</i>',
                '\\\\skt': '',
                'acorr': '<corr>ac</corr>',
                'pcorr': '<corr>pc</corr>',
                '\\\\-': '',
                '\\\\oo': " •", 
                '\\\\uncl': "",
                '\\\\kb': "≈",
                '\\\\unmetrical': "(<i>unmetrical</i>)",
                '\\\\unmetr': "(<i>unmetrical</i>)",
                '\\\\cancelled': "(<i>cancelled</i>)",
                '\\Ł': '<SKT>', 
                '\\$': '</SKT>',
                '<UNCL>': '≀',
                '</UNCL>': '≀',
                '<MNTR>': '<b>',
                '</MNTR>': '</b>',
                '<EYESKIPTO>': '(<i>eyeskip ',
                '</EYESKIPTO>': '</i>)',
                '--': '–',
                'Ó': 'oṃ',
                '\\\\': '', 
                '{': '', 
                '}': '' 
                } 
        for c in subdict:
            line = re.sub(c, subdict[c], line)
        return line


